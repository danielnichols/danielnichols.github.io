  "scripts": {
    "===== Alias Commands ============================================": "",
    "start": "yarn start:dev",
    "build": "yarn build:prod",
    "===== Main Commands =============================================": "",
    "start:prod": "cross-env NODE_ENV=production node scripts/devServer.js",
    "start:dev": "cross-env NODE_ENV=development node scripts/devServer.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.prod.js --colors",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./config/webpack/webpack.config.dev.js --colors",
    "lint:scripts": "eslint .",
    "lint:css": "stylelint \"./src/**/*.@(js|jsx|ts|tsx)\"",
    "deploy": "gh-pages -d build -b master -s \"{./!(analysis)/**,./!(asset-manifest.json)}\"",
    "===== Technical Commands ========================================": "",
    "clean": "yarn clean:build && yarn clean:cache",
    "clean:build": "rimraf build",
    "clean:cache": "yarn clean:cache:babel && yarn clean:cache:eslint && yarn clean:cache:webpack && yarn clean:cache:terser && yarn clean:cache:imagemin && yarn clean:cache:gh-pages",
    "clean:cache:babel": "rimraf node_modules/.cache/babel-loader",
    "clean:cache:eslint": "rimraf node_modules/.cache/eslint-loader",
    "clean:cache:webpack": "rimraf node_modules/.cache/cache-loader",
    "clean:cache:terser": "rimraf node_modules/.cache/terser-webpack-plugin",
    "clean:cache:imagemin": "rimraf node_modules/.cache/imagemin-plugin",
    "clean:cache:gh-pages": "rimraf node_modules/.cache/gh-pages",
    "===== pre/post Commands =========================================": "",
    "predeploy": "yarn clean:cache:gh-pages && yarn build:prod",
    "prebuild:prod": "yarn clean:build",
    "prebuild:dev": "yarn clean:build"
  }
